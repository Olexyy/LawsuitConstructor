@model List<HtmlString>

@{
    ViewBag.Title = MVC.Translate.It("Preview");
    int count = Model.Count;
}

<h2> @ViewBag.Title </h2>


@using (Html.BeginForm()) { @Html.AntiForgeryToken()  @Html.ValidationSummary(true)
<fieldset style="background-color:#fff">
        <div class="editor-field">             
            @if (count == 0)
            {
                <p> @MVC.Translate.It("Block has no text parts") </p>   
            }
            else
            {
                foreach(HtmlString part in Model)
                {
                    @part
                }
            }
        </div>
    </fieldset>
}
<div>
    <a href="javascript:history.back()">@MVC.Translate.It("Back")</a>
</div>
