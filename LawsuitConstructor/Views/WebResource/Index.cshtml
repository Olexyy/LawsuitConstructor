@model List<MVC.Models.WebResource>
@{
    ViewBag.Title = MVC.Translate.It("Site web resources");
    string message = (string)TempData["Message"];
}
<h2> @ViewBag.Title </h2>
<h4> @message </h4>

<div id="view-zone">
    <ul>
    @if(Model.Count == 0){
        <p> Список пустий </p>
    }
    @foreach(var webResource in Model){
        <li> @Html.ActionLink(MVC.Translate.It(@webResource.WebResourceKey), "Edit", "WebResource", new { webResourceId = webResource.WebResourceId }, null) </li>
    }
    </ul>
</div>
<p>
    @Html.ActionLink(MVC.Translate.It("Add new"), "Add", "WebResource") <br />
    @Html.ActionLink(MVC.Translate.It("Administrative menu"), "Admin", "Admin")
</p>
